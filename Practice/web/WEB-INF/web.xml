<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>home.html</welcome-file>
    </welcome-file-list>
    <!-- Register Servlet -->
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.practice.RegisterServlet</servlet-class>
    </servlet>
    <!-- Success Servlet -->
    <servlet>
        <servlet-name>SuccessServlet</servlet-name>
        <servlet-class>com.practice.SuccessServlet</servlet-class>
    </servlet>
    <!-- Sum Servlet -->
    <servlet>
        <servlet-name>SumServlet</servlet-name>
        <servlet-class>com.attributes.SumServlet</servlet-class>
    </servlet>
    <!-- Product Servlet -->
    <servlet>
        <servlet-name>ProductServlet</servlet-name>
        <servlet-class>com.attributes.ProductServlet</servlet-class>
    </servlet>
    
    <!-- Register Servlet Mapping -->
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <!-- Success Servlet Mapping -->
    <servlet-mapping>
        <servlet-name>SuccessServlet</servlet-name>
        <url-pattern>/success</url-pattern>
    </servlet-mapping>
    <!-- Sum Servlet Mapping -->
    <servlet-mapping>
        <servlet-name>SumServlet</servlet-name>
        <url-pattern>/sum</url-pattern>
    </servlet-mapping>
    <!-- Product Servlet Mapping -->
    <servlet-mapping>
        <servlet-name>ProductServlet</servlet-name>
        <url-pattern>/product</url-pattern>
    </servlet-mapping>
    <!-- Configs -->
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
